{% for section_dict in item.value %}
{% for section, options in section_dict.iteritems() %}
[{{ section }}]
{% for opt in options %}
{% for key, val in opt.iteritems() %}
{% if val is string or val is not iterable %}
{{ key }}={{ val }}
{% else %}
{% for subval in val %}
{{ key }}={{ subval }}
{% endfor %}
{% endif %}
{% endfor %}
{% endfor %}

{% endfor %}
{% endfor %}
